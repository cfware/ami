	},
	"homepage": "https://github.com/cfware/ami#readme",
	"dependencies": {},
	"devDependencies": {
		"@cfware/lint": "^1.0.1",
		"@cfware/nyc": "^0.6.0",
		"@istanbuljs/esm-loader-hook": "^0.1.0",
		"cross-env": "^7.0.0",
		"glob": "^7.1.6",
		"is-ci": "^2.0.0",
		"libtap": "^0.3.0",
		"nyc": "^15.0.0",
		"p-map": "^4.0.0",
